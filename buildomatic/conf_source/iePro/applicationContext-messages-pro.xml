<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.1.xsd">

    <bean id="bundlePathsListPro" class="com.jaspersoft.jasperserver.api.common.util.spring.ArrayFactoryBean" parent="bundlePathsListCe" >
        <property name="sourceList">
            <list merge="true">
                <value>${resource.root}/bundles/adhoc_masks</value>
                <value>${resource.root}/bundles/adhoc_messages</value>
                <value>${resource.root}/bundles/jasperreports_highcharts_messages</value>
                <value>${resource.root}/bundles/createsldatasource_messages</value>
                <value>${resource.root}/bundles/domain_designer_messages</value>
                <value>${resource.root}/bundles/LicenseMessages</value>
                <value>${resource.root}/bundles/logger_descriptions_pro</value>
                <value>${resource.root}/bundles/mondrian_exception_messages</value>
                <value>${resource.root}/bundles/pro_nav_messages</value>
                <value>${resource.root}/bundles/querybuilder_messages</value>
                <value>${resource.root}/bundles/report_option_messages</value>
                <value>${resource.root}/bundles/scheduling</value>
                <value>${resource.root}/bundles/semanticLayer</value>

                <value>${resource.root}/bundles/image_descriptions_messages</value>
                <value>${resource.root}/internal/jasperserver-pro</value>

                <!--New Bundles-->
                <value>${resource.root}/bundles/HomeBundle</value>
                <value>${resource.root}/bundles/AdHocFiltersBundle</value>
            </list>
        </property>
    </bean>
</beans>
